---
import About from '@/views/Home/components/About';
import Contact from '@/views/Home/components/Contact';
import Home from '@/views/Home/components/Home';
import Skills from '@/views/Home/components/Skills';
import Works from '@/views/Home/components/Work';

const { SERVICE_ID, TEMPLATE_ID, PUBLIC_KEY } = import.meta.env;
---

<Home />
<About />
<Skills />
<Works client:idle />
<Contact
  serviceId={SERVICE_ID}
  templateId={TEMPLATE_ID}
  publicKey={PUBLIC_KEY}
  client:idle
/>
